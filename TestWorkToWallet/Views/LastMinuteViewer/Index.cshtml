@{
    ViewData["Title"] = $"Last {ViewData["Interval"]} Minutes Viewer";
    var interval = ViewData["Interval"];
}

<h2>Last @ViewData["Interval"] Minute Messages</h2>

<div class="lastMinuteViewer" data-interval="@interval">
    <button id="loadMessagesButton" class="btn btn-primary">Load Messages</button>
    <div id="messages" class="mt-3"></div>
</div>

@section Scripts {
    <script src="~/js/lastMinuteViewer.js" asp-append-version="true"></script>
}


